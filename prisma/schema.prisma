// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("MYSPACE_DATABASE_URL_MAIN")
  shadowDatabaseUrl = env("MYSPACE_DATABASE_URL_SHADOW")
}
